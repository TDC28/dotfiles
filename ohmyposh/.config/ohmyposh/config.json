{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "python",
          "style": "plain",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}({{ .Venv }}) {{ end }}{{ end }}"
        },
        {
          "type": "session",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} | "
        },
        {
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "ó°‰‹  {{ .Path }}",
          "type": "path",
          "foreground": "#b8bb26"
        },
        {
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " | git:(</>{{ .HEAD }})</>",
          "type": "git"
        },
        {
          "foreground": "#cc241d",
          "style": "plain",
          "template": " \u2717",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
